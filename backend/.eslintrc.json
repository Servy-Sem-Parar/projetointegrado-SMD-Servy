{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "prefer-arrow-callback": "warn",
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "no-shadow": "off",
      "arrow-body-style": "off",
      "class-methods-use-this": "off",
      "linebreak-style": "off",
      "no-param-reassign": "off"
    }
}
